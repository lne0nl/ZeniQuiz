{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "format": {
      "outputs": []
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["build/**", "dist/**"]
    },
    "build-only": {
      "dependsOn": ["^build-only"],
      "outputs": ["dist/**"]
    },
    "start": {
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "e2e:test": {
      "dependsOn": ["^build-only"]
    }
  }
}
